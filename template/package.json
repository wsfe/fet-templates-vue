    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "sinon": "^7.2.3",
    "sinon-chai": "^3.3.0",
    "vue-template-compiler": "^2.5.21",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.4.0"
  }
