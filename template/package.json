{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8080",
    "build": "vue-cli-service build --server=online",
    "build:beta": "vue-cli-service build --server=beta",
    "build:report": "vue-cli-service build --report",
    "pack": "vue-cli-service build --mode joint",
    "pack:dev1": "npm run pack --server=dev1",
    "sync:dev1": "npm run pack:dev1 && fet sync dev1",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit --require tests/unit/index.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dayjs": "^1.7.0",
    "highlight.js": "^9.12.0",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-plugin-unit-mocha": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.1.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "faker": "^4.1.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
